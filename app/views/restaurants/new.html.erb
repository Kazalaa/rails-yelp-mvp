<%= simple_form_for(@restaurant) do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :category %>
  <%= f.submit "Submit restaurant", class: "btn btn-outline-success" %>
<% end %>
<br>
<%= link_to "Back to restaurants lists", restaurants_path, class: "btn btn-outline-primary" %>
